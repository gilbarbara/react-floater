    "is-lite": "^0.2.2",
    "popper.js": "^1.14.6",
    "react-proptype-conditional-require": "^1.0.4",
    "tree-changes": "^0.4.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-external-helpers": "7.2.0",
    "@babel/plugin-proposal-class-properties": "7.3.0",
